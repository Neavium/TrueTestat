<form id="ListForm">
    <a class="DueTo">{{dueUntilDate}}</a>
    <label class="FinishedLabel" for="FinishedCheckbox">Done
        <input id="FinishedCheckbox" type="checkbox" {{done}}>
    </label>
    <label class="NoteTitle" for="TextArea">{{noteTitle}}</label>
    <textarea id="TextArea">{{noteContent}}</textarea>
    <a class="Importance">{{importance}}</a>
    <input type="submit" formmethod="get" formaction="/note/editNote/{{_id}}/{{theme}}" class="Button Edit" value="Edit">
</form>
